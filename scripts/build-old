#!/bin/bash

set -x

tagNumber=`git branch | grep -e "^*" | cut -d' ' -f 2`
echo "tag number:"
echo "$tagNumber"

# npm run test-travis &&

git checkout master &&
git merge "$tagNumber" &&
npm run clean &&
npm run build &&
git add . &&
git commit -m 'auto-build on `date +%Y%m%d`' ;
git push origin master &&
git checkout "$tagNumber" &&

echo "sedux_eact build ok"
