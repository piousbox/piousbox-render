#!/bin/bash

set -x

# npm run test-travis &&

git checkout deploy &&
git merge master &&
npm run clean &&
npm run build &&
./scripts/version_bump &&
git add . &&
git commit -m 'auto-build on `date +%Y%m%d`' ;
git push origin deploy &&
npm publish &&
git checkout master &&

echo "piousbox-render build ok"
